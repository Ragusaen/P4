DigitalOutputPin p = D5

module mod{
    Int i = 8
    Time t = fib(i)
    every(t){
        set p to HIGH
        delay t
        set p to LOW
        i += 1
        t = fib(i)
    }
}

//Very ineffective
fun fib(Int n):Time{
    if(n == 0 or n == 1)
        return 1ms
    return fib(n-1) + fib(n-2)
}
